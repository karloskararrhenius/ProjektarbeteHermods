
@using ProjektHermods.Models;
@{
    ViewBag.Title = "Index";
}

<h2>Drink</h2>

@using (ReceptTipsContext context = new ReceptTipsContext())
{
    foreach (Recept item in context.Recepts)
    {
        if (item.ChoosenTypes.Typ == "Dricka")
        {
            <p>@item.Name</p>
        }
    }
}